            if (current.getDay() === 6) {
                starttime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 9, 0, 0, 0);
                endtime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 18, 30, 0, 0);
            }
            //วันปกติ
            if (current.getDay() > 0 && current.getDay() < 6) {
                starttime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 9, 0, 0, 0);
                endtime = new Date(current.getFullYear(), current.getMonth(), current.getDate(), 19, 0, 0, 0);
            }
            // คำนวนเวลา
            if ((current.getDay() > 0 && current > starttime && current < endtime) && dataFromApi === false) {
                console.log('เวลาทำการ');
                window.fbAsyncInit = function () {
                    FB.init({
